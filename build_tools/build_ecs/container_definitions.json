[
  {
    "name": "traefik",
    "image": "${aws_account_id}.dkr.ecr.ap-northeast-1.amazonaws.com/traefik:latest",
    "logConfiguration": {
              "logDriver": "awslogs",
              "options": {
      "awslogs-group": "ECSDefaultLogs",
      "awslogs-region": "ap-northeast-1",
      "awslogs-stream-prefix": "traefik-prod"
              }
    },
    "memoryReservation": 400,
    "cpu": 512,
    "portMappings": [
              {
      "containerPort": 80
              },
              {
      "hostPort": 8080,
      "protocol": "tcp",
      "containerPort": 8080
              }
    ],
    "dockerLabels" :           {
              "traefik.enable" : "true",
              "traefik.frontend.rule" : "Host:works.middenii.com",
              "traefik.port" : "8080"
          }
      }
  ]