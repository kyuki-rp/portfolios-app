[
  {
    "name": "traefik",
    "image": "${aws_account_id}.dkr.ecr.ap-northeast-1.amazonaws.com/traefik:latest",
    "networkMode": "awsvpc",
    "memoryReservation": 256,
    "essential": true,

    "portMappings": [
      {
        "hostPort": 8080,
        "containerPort": 8080
      },
      {
        "hostPort": 443,
        "containerPort": 443
      }
    ]
  }
  ]