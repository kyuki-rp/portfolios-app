[
  {
    "name": "traefik",
    "image": "${aws_account_id}.dkr.ecr.ap-northeast-1.amazonaws.com/traefik:latest",
    "memoryReservation": 256,
    "command": [
      "--ping",
      "--ping.entrypoint=http",
      "--ecs.clusters=potobuild",
      "--ecs.exposedbydefault=false",
      "--loglevel=DEBUG"
    ],
    "portMappings": [
              {
      "containerPort": 80
              },
              {
      "hostPort": 8080,
      "protocol": "tcp",
      "containerPort": 8080
              }
    ],
    "dockerLabels" :           {
              "traefik.enable" : "true",
              "traefik.frontend.rule" : "Host:works.middenii.com",
              "traefik.port" : "8080"
          }
      }
  ]